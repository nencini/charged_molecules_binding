unset multiplot
set size 1,1
set terminal pdf enhanced size 10,7.5
set encoding utf8
set output "tpp_binding_main.pdf"
 
set yrange [0:3]
set ytics format "%.2f" font 'Times,16'
 
set key inside left maxcols 2
set key reverse
set key Left

      


#set label format small
set datafile commentschar "@#$%&"
set multiplot layout 3,3 


set xlabel "Time [nm]" font 'Times,16'
set ylabel "Bound particles"  font 'Times,16'

set style fill transparent solid 0.25 # partial transparency
set style fill noborder # no separate top/bottom lines




set xtics 500 font 'Times,16'
set xrange [0:]
set yrange [0:40]
set title "140mM, ProteinFF" font 'Times,20'

